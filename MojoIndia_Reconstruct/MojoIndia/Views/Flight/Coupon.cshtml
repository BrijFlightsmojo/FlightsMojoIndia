<main>
    

    <section class="banner_section" style="background:#6dc0e5;    position: relative;">
        <div class="container ">
            <div class="row ">


              @{ Html.RenderPartial("~/Views/Shared/SearchEngine.cshtml");}


            </div>
        </div>
    </section>

    <div class="trust_container">
        <div class="container">
            <picture>
                <source srcset="/images/travel_icon.webp" type="image/webp" />

                <img loading="lazy" src="/images/travel_icon.png" alt="banner" />
            </picture>


        </div>
    </div>

    <div class="couponmain_Con">
        <div class="container">
            <div class="row">
                <div class="coupon_leftCon">
                    <div class="coupon_leftTop">
                       
                        <div class="dicount_heading">
                            <h1>Explore Your Beloved Destination with Zero Convenience Fees. Make haste now!</h1>
                        </div>
                        <div class="discont_desg">
                            <div class="dicount_code">
                                <div class="Code_Top">Promo Code</div>                      

                                <span class="cop_Dashed"></span>
                                <div class="Code_Bottom">INSTAMOJO <a href="#"><img src="~/images/copywhite.svg"/></a></div>
                            </div>
                        </div>

                        
                    </div>
                    <div class="clearfix"></div>
                    <div class="discount_content">
                        <h2>What's in it for you?</h2>
                        <ul>
                            <li>Flight bookings without any 'Convenience Fees'.</li>
                        </ul>
                    </div>
                    <div class="discount_content">
                        <h2>How to Claim Your Discount?</h2>
                        <ul>
                            <li>To enjoy the discount, simply insert Promo Code: INSTAMOJO in the coupon field when making your flight bookings</li>
                            <li>This offer is applicable to bookings made on Flightsmojo's website alone.</li>
                        </ul>
                    </div>
                    <div class="discount_content">
                        <h2>Are there any other important facts to be aware of?</h2>
                        <p>Additional Information to Keep in Mind:</p>
                        <ul>
                            <li>This offer is exclusively applicable to reservations made using the designated promo codes.</li>
                            <li>This promotion cannot be clubbed with any other ongoing offers on Flightsmojo.</li>
                            <li>If there are any partial or full cancellations, a convenience fee of Rs. 250 per passenger per sector will be applicable.</li>
                        </ul>
                    </div>
                    <div class="discount_content">
                        <h2>Terms & Conditions:</h2>
                       
                        <ul>
                            <li>Discounts availed under this Offer cannot be clubbed with any other offer</li>
                            <li>Participants of this Offer may be contacted for marketing and promotional purposes.</li>
                            <li>You acknowledge and confirm that You are aware of the nature of telecommunications/ internet services and that an email transmission may not be received properly and may be read or be known to any unauthorized persons. You agree to assume and bear all the risks involved in respect of such errors and misunderstandings and Blinc shall not be responsible in any manner for the same.</li>
                            <li>This Offer is valid only in India.</li>
                            <li>The Offer is open to only end consumers and not resellers.</li>
                            <li>Offer benefits/Discounts shall not be settled in cash and are not transferable.</li>
                            <li>Blinc reserves the right to terminate, withdraw, suspend, amend, extend, modify, add or remove portions of this Offer, at any time without any prior written notice.</li>
                        </ul>
                    </div>
                </div>
                <div class="coupon_RightCon">
                    <div class="coupon_banner">
                        <span>INSTAMOJO <a href="#"><img src="/images/copy.svg" /></a></span>
                        <img src="/images/int-offer.png" alt="coupon" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="trust_pilot_con">
        <div class="container">
            <div class="row">
                <div class="tp_container">
                    <div data-romw-token="ck15c9nAI4hMGt68Hr18kHA0dMpYudzU5xxakB1kyMPLUkZFz2"></div>
                    <div data-romw-token="bf0ae0CVfZ448bq1qbjgbZ6VRnBpqA1dOabBBE7wp311G4zhrP"></div>

                </div>
            </div>
        </div>
    </div>


    
    


</main>
@section scripts {
    <script>
    $('.showBtnfaq').click(function () {

        if ($(this).hasClass('active')) {
            $(this).removeClass('active');
            $('.hidemefaq').slideUp();
        } else {
            $('.hidemefaq').slideUp();
            $('.showBtnfaq').removeClass('active');
            $(this).addClass('active');
            $(this).next().filter('.hidemefaq').slideDown();
        }
    });
    </script>
